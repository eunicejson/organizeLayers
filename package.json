{
  "name": "organizelayers",
  "description": "A plugin that allows short key commands to move layers up, down and reverse positions.",
  "version": "0.2.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "organizeLayers",
    "manifest": "src/manifest.json",
    "main": "organizelayers.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Eunice Son <eunicejson@gmail.com>",
  "repository": "https://github.com/eunicejson/organizeLayers",
  "dependencies": {
    "skpm": "^1.2.0"
  }
}
